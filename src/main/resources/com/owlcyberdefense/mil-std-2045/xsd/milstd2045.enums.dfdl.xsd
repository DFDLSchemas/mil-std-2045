    <restriction base="ms2045:enumString">
      <enumeration value="OK_Link_16" dfdlx:repValues="0"/>
      <enumeration value="OK_Binary_File" dfdlx:repValues="1"/>
      <enumeration value="OK_VMF" dfdlx:repValues="2"/>
      <enumeration value="OK_NITFS" dfdlx:repValues="3"/>
      <enumeration value="OK_RDM" dfdlx:repValues="4"/>
      <enumeration value="OK_USMTF" dfdlx:repValues="5"/>
      <enumeration value="OK_USMTF-DOI-103" dfdlx:repValues="6"/>
      <enumeration value="OK_XML-MTF" dfdlx:repValues="7"/>
      <enumeration value="OK_XML-VMF" dfdlx:repValues="8"/>
      <!--
      This pattern facet insures that only the above "OK" values are
      considered valid.
      -->
      <pattern value="OK_[0-9A-Za-z_-]{0,20}"/>
      <!--
      The remaining enums don't have OK_ prefixes, so will be
      considered well-formed, but won't pass validation, as
      both the pattern AND one of the enumerations must be
      satisfied.
      -->
      <enumeration value="Undefined_09" dfdlx:repValues="9"/>
      <enumeration value="Undefined_10" dfdlx:repValues="10"/>
      <enumeration value="Undefined_11" dfdlx:repValues="11"/>
      <enumeration value="Undefined_12" dfdlx:repValues="12"/>
      <enumeration value="Undefined_13" dfdlx:repValues="13"/>
      <enumeration value="Undefined_14" dfdlx:repValues="14"/>
      <enumeration value="Undefined_15" dfdlx:repValues="15"/>
    </restriction>
  </simpleType>

