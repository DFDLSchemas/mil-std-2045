    <restriction base="ms2045:enumString">
      <enumeration value="OK_6016" dfdlx:repValues="0"/>
      <enumeration value="OK_6016A" dfdlx:repValues="1"/>
      <enumeration value="OK_6016B" dfdlx:repValues="2"/>
      <enumeration value="OK_6016C" dfdlx:repValues="3"/>
      <enumeration value="OK_6016D" dfdlx:repValues="4"/>
      <enumeration value="OK_6016E" dfdlx:repValues="5"/>
      <enumeration value="OK_6016F" dfdlx:repValues="6"/>
